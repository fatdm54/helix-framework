# OpenClaw Agent Framework - 环境变量配置

# 复制此文件为 .env 并填入你的配置

# ===========================================
# OpenAI
# ===========================================
OPENAI_API_KEY=sk-your-openai-key-here

# ===========================================
# Anthropic (Claude)
# ===========================================
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here

# ===========================================
# GitHub
# ===========================================
GH_TOKEN=ghp_your-github-token-here

# ===========================================
# Agent 模型配置
# ===========================================
HELIX_MODEL=gpt-4o
CODEX_MODEL=codex
RESEARCHER_MODEL=gpt-4o
REVIEWER_MODEL=claude
TESTER_MODEL=gpt-4o
DOCUMENTER_MODEL=gpt-4o
SCHEDULER_MODEL=gpt-4o
EVALUATOR_MODEL=claude
LEARNING_MODEL=gpt-4o
GUARDIAN_MODEL=gpt-4o

# ===========================================
# 日志配置
# ===========================================
LOG_LEVEL=info
LOG_FILE=/app/logs/openclaw.log

# ===========================================
# 其他配置
# ===========================================
TZ=Asia/Kuala_Lumpur
